public class Lesson {

String Name;
Double Grade;
int Quarter;
int ff;

 public Lesson(String Name, Double Grade, int Quarter) {

 this.Name = Name;
 this.Grade = Grade;
 this.Quarter = Quarter;
 

 }

 public String getName() {

  return Name;
 }

 public void setName(String Name) {

  this.Name = Name;
 }

 public Double getGrade() {

  return Grade;

 }

 public void setGrade(Double Grade) {

   this.Grade = Grade;

 }

 public int getQuarter() {

  return Quarter;

 }

 public void setQuarter(int Quarter) {

  this.Quarter = Quarter;

  }
  }
